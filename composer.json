{
    "name": "jmikola/insecure-routes-bundle",
    "type": "symfony-bundle",
    "description": "Removes HTTPS scheme requirements from routes in your Symfony2 app (for dev/test environments).",
    "keywords": ["https", "routes", "routing", "security"],
    "homepage": "https://github.com/jmikola/JmikolaInsecureRoutesBundle",
    "license": "MIT",
    "authors": [
        { "name": "Jeremy Mikola", "email": "jmikola@gmail.com" }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/config": "~2.1",
        "symfony/dependency-injection": "~2.1",
        "symfony/http-kernel": "~2.1",
        "symfony/routing": "~2.1"
    },
    "autoload": {
        "psr-0": { "Jmikola\\InsecureRoutesBundle": "" }
    },
    "target-dir": "Jmikola/InsecureRoutesBundle"
}
